# MongoDB
MONGO_URI=mongodb://user:password@host:port/database?authSource=admin

# MySQL RDS
CNN_MYSQL_DB_HOST="your-rds-endpoint.rds.amazonaws.com"
CNN_MYSQL_DB_USER="username"
CNN_MYSQL_DB_PASSWORD="password"
CNN_MYSQL_DB_NAME="database"
CNN_MYSQL_DB_PORT="3306"

# AWS Credentials (APENAS para desenvolvimento local)
# Em produção (EC2), usar IAM Role em vez dessas credenciais
REGION="us-east-1"
ACCESS_KEY_ID=your-access-key-id
SECRET_ACCESS_KEY=your-secret-access-key
SESSION_TOKEN=your-session-token

# CloudWatch Logs
LOG_GROUP_NAME="your-log-group"
LOG_STREAM_NAME="your-log-stream"

# Environment (production para usar IAM Role)
NODE_ENV=development
